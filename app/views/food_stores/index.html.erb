<p id="notice"><%= notice %></p>

<h1>Food Stores</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Zipcode</th>
      <th>City</th>
      <th>Streetaddress</th>
      <th>Openhours</th>
      <th>Closehours</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_stores.each do |food_store| %>
      <tr>
        <td><%= food_store.name %></td>
        <td><%= food_store.zipCode %></td>
        <td><%= food_store.city %></td>
        <td><%= food_store.streetAddress %></td>
        <td><%= food_store.openHours %></td>
        <td><%= food_store.closeHours %></td>
        <td><%= link_to 'Show', food_store %></td>
        <td><%= link_to 'Edit', edit_food_store_path(food_store) %></td>
        <td><%= link_to 'Destroy', food_store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food Store', new_food_store_path %>
